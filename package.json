{
  "name": "repdao",
  "version": "0.1.0",
  "description": "Reputation DAO JS client + CLI wrapper (no dfx runtime)",
  "type": "module",
  "main": "dist/client.js",
  "exports": {
    ".": {
      "import": "./dist/client.js"
    }
  },
  "bin": {
    "repdao": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm src/cli.ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "src/idl/reputationchild.did.js",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@dfinity/agent": "3.2.4",
    "@dfinity/candid": "3.2.4",
    "@dfinity/identity-secp256k1": "3.2.4",
    "@dfinity/principal": "3.2.4",
    "commander": "12.1.0",
    "dotenv": "16.4.5"
  },
  "devDependencies": {
    "@types/commander": "^2.12.5",
    "@types/node": "20.14.2",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
